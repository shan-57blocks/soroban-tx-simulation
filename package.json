{
  "name": "soroban-tx-simulation",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add ."
    ]
  },
  "scripts": {
    "lint": "eslint src/**/*.ts src/**/**/*.ts",
    "lint-staged": "lint-staged",
    "format": "prettier --write .",
    "test": "jest --silent",
    "test:coverage": "jest --verbose --coverage",
    "prepare": "husky install",
    "simulation": "ts-node src/index.ts"
  },
  "dependencies": {
    "@safe-global/protocol-kit": "^5.0.1",
    "@stellar/stellar-sdk": "^12.1.0",
    "dotenv": "^8.2.0",
    "stellar-hd-wallet": "^0.0.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "4.5.5"
  }
}
